{
	"version": "0.2",
	"ignorePaths": [
		"dist",
		"node_modules",
		".git",
		"**/.DS_Store",
		"*.lock.json",
		"*-lock.json",
		"*.lock",
		"*.tgz"
	],
	"dictionaries": [
		"typescript"
	],
	// Before adding a word, see if there is already a dictionary in which it is contained.
	// http://github.com/streetsidesoftware/cspell-dicts
	"words": [
		"editorconfig",
		"eslintconfig",
		"roydukkey",
		"stroustrup",
		"tsdoc"
	]
}
