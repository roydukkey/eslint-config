{
	"version": "0.2",
	"ignorePaths": [
		"dist",
		"node_modules",
		".git",
		"**/.DS_Store",
		"*-lock.yaml",
		"*.tgz"
	],
	"enableGlobDot": true,
	"dictionaries": [
		"typescript"
	],
	// Before adding a words, see if there is already a dictionary in which it is contained: `npx cspell trace '{{WORD}}'`
	"words": [
		"dbaeumer",
		"editorconfig",
		"eslintconfig",
		"roydukkey",
		"stroustrup",
		"tsdoc"
	]
}
